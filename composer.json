{
    "name": "reddogs-at/reddogs-oauth2-server",
    "description": "implementation of league/oauth2-server",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "league/oauth2-server": "^5.1",
        "reddogs-at/reddogs-test": "^0.1.2",
        "reddogs-at/reddogs-doctrine": "^1.2",
        "reddogs-at/reddogs-migrations": "^0.1.0",
        "zendframework/zend-crypt": "^3.2"
    },
    "autoload": {
      "psr-4": {
        "Reddogs\\OAuth2\\Server\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ReddogsTest\\OAuth2\\Server\\": "test/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mtymek/expressive-config-manager": "^0.4.0",
        "zendframework/zend-servicemanager": "^3.2",
        "dasprid/container-interop-doctrine": "^0.2.3"
    }
}
